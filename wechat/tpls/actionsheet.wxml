<template name="actionsheet">
  <action-sheet hidden="{{hidden}}">
      <action-sheet-item
      wx:for="{{items}}"
      wx:key="index"
      data-idx="{{index}}"
      bindtap="todoItem"
      >
      
      {{item}}
      </action-sheet-item>
  
  <action-sheet-cancel bindtap="cancel">取消</action-sheet-cancel>
  </action-sheet>


</template>